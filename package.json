{
  "name": "ml-conrec",
  "version": "5.0.2",
  "description": "JavaScript implementation of the CONREC contouring algorithm",
  "main": "./lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest --coverage",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mljs/conrec.git"
  },
  "keywords": [
    "algorithm",
    "contour",
    "plot",
    "conrec",
    "contouring",
    "machine",
    "learning",
    "data",
    "mining",
    "datamining"
  ],
  "author": "MichaÃ«l Zasso",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mljs/conrec/issues"
  },
  "homepage": "https://github.com/mljs/conrec#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@types/jest": "^29.5.13",
    "eslint": "^8.38.0",
    "eslint-config-cheminfo-typescript": "^11.3.1",
    "esm": "^3.2.25",
    "jcampconverter": "^9.6.4",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "cheminfo-types": "^1.8.0"
  }
}
